{
  "name": "proshop",
  "version": "1.0.0",
  "description": "e-commerce application built with mern stack",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "start": "node backend/server.js",
    "server": "nodemon backend/server.js",
    "client": "npm run dev --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "data:import": "node backend/seeder.js",
    "data:destroy": "node backend/seeder.js -d"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/muhammedZak/proshopee.git"
  },
  "author": "Muhammed Zakariya",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/muhammedZak/proshopee/issues"
  },
  "homepage": "https://github.com/muhammedZak/proshopee#readme",
  "dependencies": {
    "bcryptjs": "^3.0.3",
    "colors": "^1.4.0",
    "express": "^5.2.1",
    "mongoose": "^9.1.1"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "dotenv": "^17.2.3",
    "nodemon": "^3.1.11"
  }
}
